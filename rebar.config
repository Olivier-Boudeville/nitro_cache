{erl_opts, [
	debug_info,
	{parse_transform, stacktrace_transform}
]}.
{cover_enabled, true}.
{xref_warnings, true}.
{xref_checks, [
  undefined_function_calls,
  undefined_functions,
  locals_not_used,
  deprecated_function_calls,
  deprecated_functions
]}.

{deps, [
	{stacktrace_compat, {git, "git://github.com/g-andrade/stacktrace_compat", {tag, "1.0.1"}}}
]}.
